x = 25

if x < 10:
    print("x is lessthan 20")
    if x < 15:
        print("x is lessthan 15")
    else:
        print("x is morethan 15")
        
else:
    print("x is morethan 20")
        
a = 1
while a<5:
    print(a)
    a += 5

for x in range(0,10):
    
    print(x, end =",")
    
nums =[x for x in range(0,10)]
for x in nums:
    if (x==4): continue
    print(x, end=",")
print("done")

