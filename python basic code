nums =[x for x in range(0,10)]
index = 0
while index < len(nums):
    print(nums[index])
    index +=1
    

def add(a,b):
    sum = a+b
    return sum

def multiply(x,y):
    r=lambda x,y: x*y
    r(12,3)

def myfunc(n):
    return lambda  a : a + n
mysum = myfunc(3)
print(mysum(10))
        
multiply = lambda x,y : x*y
print(multiply(25,15))

